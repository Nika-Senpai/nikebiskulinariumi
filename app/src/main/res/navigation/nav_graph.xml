<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/kerdzebiFragment">
    <fragment
        android:id="@+id/loginFragment3"
        android:name="com.example.nikebiskulinariumi.fragment.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment3_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment3_to_resetPasswordFragment"
            app:destination="@id/resetPasswordFragment" />
        <action
            android:id="@+id/action_loginFragment3_to_secretFragment"
            app:destination="@id/secretFragment" />
    </fragment>
    <fragment
        android:id="@+id/kerdzebiFragment"
        android:name="com.example.nikebiskulinariumi.fragment.KerdzebiFragment"
        android:label="KerdzebiFragment" >
        <action
            android:id="@+id/action_kerdzebiFragment_to_loginFragment3"
            app:destination="@id/loginFragment3" />
        <action
            android:id="@+id/action_kerdzebiFragment_to_chalagajisFragment"
            app:destination="@id/chalagajisFragment" />
        <action
            android:id="@+id/action_kerdzebiFragment_to_kuchmachiFragment"
            app:destination="@id/kuchmachiFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.nikebiskulinariumi.fragment.RegisterFragment"
        android:label="RegisterFragment" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment3"
            app:destination="@id/loginFragment3" />
    </fragment>
    <fragment
        android:id="@+id/resetPasswordFragment"
        android:name="com.example.nikebiskulinariumi.fragment.ResetPasswordFragment"
        android:label="ResetPasswordFragment" >
        <action
            android:id="@+id/action_resetPasswordFragment_to_loginFragment3"
            app:destination="@id/loginFragment3" />
    </fragment>
    <fragment
        android:id="@+id/secretFragment"
        android:name="com.example.nikebiskulinariumi.fragment.SecretFragment"
        android:label="SecretFragment" >
        <action
            android:id="@+id/action_secretFragment_to_thirdSecretFragment"
            app:destination="@id/thirdSecretFragment" />
    </fragment>
    <fragment
        android:id="@+id/chalagajisFragment"
        android:name="com.example.nikebiskulinariumi.fragment.ChalagajisFragment"
        android:label="ChalagajisFragment" />
    <fragment
        android:id="@+id/thirdSecretFragment"
        android:name="com.example.nikebiskulinariumi.fragment.ThirdSecretFragment"
        android:label="ThirdSecretFragment" />
    <fragment
        android:id="@+id/kuchmachiFragment"
        android:name="com.example.nikebiskulinariumi.fragment.KuchmachiFragment"
        android:label="KuchmachiFragment" />
</navigation>